{
  "metadata": [
    {
      "src": [
        {
          "src": ".",
          "files": [
            "Horiba.Sdk/Horiba.Sdk.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**"
          ]
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [
      "ExtractSearchIndex"
    ],
    "globalMetadata": {
      "_appTitle": "HORIBA EzSpec SDK Documentation",
      "_appName": "HORIBA EzSpec SDK",
      "_enableSearch": true,
      "_gitContribute": {
        "repo": "https://github.com/HORIBAEzSpecSDK/dotnet-framework-sdk",
        "branch": "feature/threadSafety"
      },
      "_gitUrlPattern": "github"
    },
    "dest": "_site",
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}
